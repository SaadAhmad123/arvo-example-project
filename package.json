{
  "name": "typescript-starter-project",
  "description": "This is a starter project template for a typescript project",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "start": "tsup && node dist/index.cjs",
    "lint": "biome check --fix",
    "format": "biome format --fix",
    "otel": "docker compose up"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "just-install": "^2.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=19"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@arizeai/openinference-semantic-conventions": "^2.1.2",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.2",
    "@opentelemetry/context-zone": "^2.1.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.205.0",
    "@opentelemetry/instrumentation": "^0.205.0",
    "@opentelemetry/instrumentation-document-load": "^0.50.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/sdk-trace-web": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "arvo-core": "^3.0.6",
    "arvo-event-handler": "^3.0.6",
    "dotenv": "^17.2.2",
    "openai": "^5.22.0",
    "xstate": "^5.22.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  }
}
